<script>
function creat(){
    var result = new Array();

    for(var i=0; i<10; i++){
        result[i] = function(num){
            return function(){
                return num
            };
        }(i);
    }

    return result;
}
creat();

var funca = creat();

for(var i = 0; i < funca.length; i++) {
    console.log(funca[i]())
}
</script>


